{
  "name": "react-offline-test",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest **/*.test.js*",
    "start": "./node_modules/.bin/webpack-dev-server --hot --progress --colors"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.1",
    "@nivo/core": "^0.59.1",
    "@nivo/pie": "^0.59.1",
    "@types/moment": "^2.13.0",
    "enzyme": "^3.10.0",
    "moment": "^2.24.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/helpers": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.18",
    "@types/react-dom": "^16.9.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "enzyme-adapter-react-16": "^1.15.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0"
  },
  "jest": {
    "testRegex": "\\.(test|spec)\\.(j|t)sx?$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
